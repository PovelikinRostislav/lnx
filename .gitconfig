[core]
    editor = vim
[http]
    proxy = SET_PROXY
[user]
    name = SET_NAME
    email = SET_EMAIL
[alias]
    st = status
    co = checkout
    br = branch
    resetlast = reset --hard HEAD~1
    lg = log --all --graph --abbrev-commit --decorate --format=format:'%C(bold blue)%h%C(reset) - %C(bold green)[%aD]%C(reset) %C(white)%s%C(reset) %C(dim white)- %an%C(reset)%C(bold yellow)%d%C(reset)'
    last = !"git lg -n 5"
