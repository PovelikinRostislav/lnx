[global_config]
  borderless = True
  tab_position = bottom
  title_font = Monospace 14
  title_hide_sizetext = True
  title_transmit_bg_color = "#047a7b"
  title_transmit_fg_color = "#f5f5f5"
  title_use_system_font = False
  window_state = maximise
[keybindings]
  full_screen = None
  go_down = <Alt>j
  go_left = <Alt>h
  go_next = <Alt>n
  go_prev = <Alt>p
  go_right = <Alt>l
  go_up = <Alt>k
  layout_launcher = None
  next_tab = <Shift><Alt>l
  prev_tab = <Shift><Alt>h
[layouts]
  [[default]]
    [[[child1]]]
      parent = window0
      profile = default
      type = Terminal
    [[[window0]]]
      parent = ""
      type = Window
[plugins]
[profiles]
  [[default]]
    background_image = None
    copy_on_selection = True
    custom_command = /bin/bash --rcfile /swgwork/rostislavp/configs/.bashrc
    font = Monospace 14
    scrollback_infinite = True
    scrollbar_position = hidden
    use_system_font = False
